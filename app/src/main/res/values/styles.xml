<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorProgression">@color/colorProgression</item>
        <item name="colorProgression2">@color/colorProgression2</item>
        <item name="android:colorBackground">@color/colorBackground</item>
        <item name="colorContrasted">@color/colorDarkkkk</item>
        <item name="colorListItems">@color/colorListItems</item>
        <item name="colorNavBars1">@color/colorWhite</item>
        <item name="colorNavBars2">@color/colorGrey</item>
        <item name="colorBackground">@color/colorBackground</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:textColorPrimary">@color/colorDarkkkk</item>
        <item name="android:textColorSecondary">@color/colorBackgroundDark</item>
    </style>

